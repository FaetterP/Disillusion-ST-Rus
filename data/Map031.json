            { "code": 401, "indent": 0, "parameters": ["Always looking for a way to bring out the sadness "] },
            { "code": 401, "indent": 0, "parameters": ["and tragedy in everything.\\. "] },
            { "code": 101, "indent": 0, "parameters": ["", 0, 1, 2, ""] },
            { "code": 401, "indent": 0, "parameters": ["Not my favorite company,\\. I must say. "] },
            { "code": 101, "indent": 0, "parameters": ["", 0, 1, 2, ""] },
            { "code": 401, "indent": 0, "parameters": ["Listen, friend, I can tell you're eager to buy something,\\. "] },
